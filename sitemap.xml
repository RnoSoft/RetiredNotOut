---
layout: null
no-index: true;
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%- for page in site.pages -%}
        {%- if site.no-index -%}
            {%- continue -%}
        {%- endif -%}
        {%- if page.no-index -%}
            {%- continue -%}
        {%- endif -%}
         {%- if page.url == "/index" -%}
            {%- assign _url = "" -%}
        {%- else -%}
            {%- assign _url = page.url -%}
        {%- endif %}
        <url><loc>https://retirednotout.uk{{_url}}</loc></url>
    {%- endfor %}
    {%- for page in site.posts -%}
        {%- if site.no-index -%}
            {%- continue -%}
        {%- endif -%}
        {%- if page.no-index -%}
            {%- continue -%}
        {%- endif -%}
         {%- if page.url == "/index" -%}
            {%- assign _url = "" -%}
        {%- else -%}
            {%- assign _url = page.url -%}
        {%- endif %}
        <url><loc>https://retirednotout.uk{{_url}}</loc></url>
    {%- endfor %}
</urlset>
